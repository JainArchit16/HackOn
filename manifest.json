{
    "name": "Amazon Hackon",
    "description": "this is a chrome extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["scripting", "activeTab", "notifications","tabs","storage"],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
  }
  